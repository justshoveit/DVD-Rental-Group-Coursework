@model List<SAMS.Models.StudentReport>

<div>
    <h4>Student Report</h4>
    <hr />
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Student Name</th>
                <th>Roll Number</th>
                <th>Enroll Date</th>
                <th>Course Name</th>
                <th>Semester</th>
            </tr>
        </thead>
        <tbody>
            @for (var i = 0; i < Model.Count; i++)
            {
                <tr>
                    <td>@Html.DisplayFor(model => model[i].StudentName)</td>
                    <td>@Html.DisplayFor(model => model[i].RollNumber)</td>
                    <td>@Html.DisplayFor(model => model[i].EnrollDate)</td>
                    <td>@Html.DisplayFor(model => model[i].CourseName)</td>
                    <td>@Html.DisplayFor(model => model[i].SemesterName)</td>
                </tr>
            }
        </tbody>
    </table>
</div>
<p>
    @Html.ActionLink("Back to List", "Index")
</p>
